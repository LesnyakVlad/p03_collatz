   "source": [
    "from tqdm import tqdm\n",
    "\n",
    "\n",
    "def collatz(n):\n",
    "    return (n / 2) if n % 2 == 0 else 3 * n + 1\n",
    "\n",
    "\n",
    "def check(n):\n",
    "    while n != 1:\n",
    "        n = collatz(n)\n",
    "\n",
    "\n",
    "for i in tqdm(range(1000000)):\n",
    "    check(i)"
   ]
  }
